<launch>
  <param name = "robot_description" command ="cat '$(find mono)/urdf/mono.urdf'"/>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find mono)/config/position_controller.yaml" command="load"/>
  
  <node name = "mono_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
        args=" -urdf -param robot_description -model mybot -x 0 -y 0 -z 0.764"/>
  

  <!-- load the controllers -->
 <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="/mono" args="joint_state_controller l_hip_yaw_position	l_hip_roll_position	l_hip_pitch_position l_knee_pitch_position l_ankle_pitch_position l_ankle_roll_position	r_hip_yaw_position r_hip_roll_position r_hip_pitch_position	r_knee_pitch_position r_ankle_pitch_position r_ankle_roll_position" />

  <!-- convert joint states to TF transforms for rviz, etc -->

</launch>
